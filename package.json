    "sqlite3": "^5.1.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/node": "^22.10.6",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.1",
    "supertest": "^7.0.0",
    "tsup": "^8.3.5",
